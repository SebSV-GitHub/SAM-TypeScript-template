	"license": "MIT",
	"bugs": {
		"url": "https://github.com/SebSV-GitHub/SAM-TypeScript-template/issues"
	},
	"homepage": "https://github.com/SebSV-GitHub/SAM-TypeScript-template#readme",
	"engines": {
		"node": ">=22"
	},
	"devDependencies": {
		"@tsconfig/node22": "^22.0.0",
