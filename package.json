		"@types/node": "^22.13.4",
		"ava": "^6.2.0",
		"c8": "^10.1.3",
		"husky": "^9.1.6",
		"lint-staged": "^15.2.10",
		"prettier": "^3.3.3",
		"tsx": "^4.19.2",
		"typescript": "^5.6.3",
